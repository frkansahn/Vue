nd);\r\n\t\t\t},\r\n\t\t\tgetReport(){\r\n\t\t\t\taxios.get('http://localhost:4001/api/v1/report' ,{'headers': { 'Authorization':'Bearer ' + localStorage.getItem('token')}})\r\n\t\t\t\t.then(response => {\r\n\t\t\t\t\tthis.orders = response.data.response;\r\n\t\t\t\t\tthis.orderLenght = response.data.response.length;\r\n\t\t\t\t\tthis.reportOrders = response.data.response;\t\r\n\t\t\t\t}).catch(err => {\r\n\t\t\t\t\tconsole.log(err);\r\n\t\t\t\t});\r\n\t\t\t},\r\n\t\t\tgetTable(){\r\n\t\t\t\taxios.get('http://localhost:4001/api/v1/table/' ,{'headers': { 'Authorization':'Bearer ' +localStorage.getItem('token')}})\r\n\t\t\t\t.then(response => {\r\n\t\t\t\t\tthis.tables = response.data.response;\r\n\t\t\t\t}).catch(err => {\r\n\t\t\t\t\tconsole.log(err);\r\n\t\t\t\t});\r\n\t\t\t},\r\n\t\t\tgetProduct(){\r\n\t\t\t\taxios.get('http://localhost:4001/api/v1/product/' ,{'headers': { 'Authorization':'Bearer ' +localStorage.getItem('token')}})\r\n\t\t\t\t.then(response => {\r\n\t\t\t\t\tthis.products = response.data.response;\r\n\t\t\t\t}).catch(err => {\r\n\t\t\t\t\tconsole.log(err);\r\n\t\t\t\t});\r\n\t\t\t},\r\n\t\t\tsetPages () {\r\n\t\t\t\tlet numberOfPages = Math.ceil(this.orders.length / this.perPage);\r\n\t\t\t\tvar newPages = [];\r\n\t\t\t\tfor (let index = 1; index <= numberOfPages; index++) {\r\n\t\t\t\t\tnewPages.push(index);\r\n\t\t\t\t}\r\n\t\t\t\tthis.pages = newPages ;\r\n\t\t\t\tthis.page = 1;\r\n\t\t\t},\r\n\t\t\tpaginate (orders) {\r\n\t\t\t\tlet page = this.page;\r\n\t\t\t\tlet perPage = this.perPage;\r\n\t\t\t\tlet from = (page * perPage) - perPage;\r\n\t\t\t\tlet to = (page * perPage);\r\n\t\t\t\tthis.FromTo =  (from + 1) + ' - ' + to + ' arasında';\r\n\t\t\t\treturn  orders.slice(from, to);\r\n\t\t\t},\r\n\t\t\tfn_selectFilter()\r\n\t\t\t{\r\n\t\t\t\tthis.orders = this.filter();\r\n\t\t\t},\r\n\t\t\tfilter(){\r\n\t\t\t\tif(this.selectTable == 0 && this.selectProduct == 0)\r\n\t\t\t\t{\r\n\t\t\t\t\treturn this.reportOrders;\t\t\t\t\t\r\n\t\t\t\t}\r\n\t\t\t\telse if(this.selectTable != 0 && this.selectProduct == 0)\r\n\t\t\t\t{\r\n\t\t\t\t\treturn this.reportOrders.filter(order => {\r\n\t\t\t\t\t\treturn order.order_log_table_id == this.selectTable;\r\n\t\t\t\t\t});\r\n\t\t\t\t}\r\n\t\t\t\telse if(this.selectTable == 0 && this.selectProduct != 0)\r\n\t\t\t\t{\r\n\t\t\t\t\treturn this.reportOrders.filter(order => {\r\n\t\t\t\t\t\treturn order.order_log_product_id == this.selectProduct;\r\n\t\t\t\t\t});\r\n\t\t\t\t}\r\n\t\t\t\telse if(this.selectTable != 0 && this.selectProduct != 0)\r\n\t\t\t\t{\r\n\t\t\t\t\treturn this.reportOrders.filter(order => {\r\n\t\t\t\t\t\treturn order.order_log_product_id == this.selectProduct && order.order_log_table_id == this.selectTable;\r\n\t\t\t\t\t});\r\n\t\t\t\t}\r\n\t\t\t\t\r\n\t\t\t},\r\n\t\t\tdownloadOrder(){\r\n\r\n\t\t\t},\r\n\t\t\tprintOrder(){\r\n\t\t\t\tvar myWindow = window.open(\"\" , \"_blank\" , \"height=700,width=700\");\r\n\r\n\t\t\t\tvar tableHtml = this.$refs.printTable;\r\n\t\t\t\tmyWindow.document.write(tableHtml.outerHTML);    \t\t\t\r\n\t\t\t\tmyWindow.print();\r\n\t\t\t\tmyWindow.close();\r\n\t\t\t}\r\n\t\t},\r\n\t\tcomputed: {\r\n\t\t\tdisplayedOrders () {\r\n\t\t\t\treturn this.paginate(this.orders);\r\n\t\t\t}\r\n\t\t},\r\n\t\twatch: {\r\n\t\t\torders () {\r\n\t\t\t\tthis.setPages();\r\n\t\t\t},\r\n\t\t\tdatetimeStart:function(newVal, oldVal)\r\n\t\t\t{\r\n\t\t\t\tif(this.datetimeStart > this.datetimeEnd)\r\n\t\t\t\t{\r\n\t\t\t\t\tthis.datetimeStart = this.datetimeEnd;\r\n\t\t\t\t}\r\n\t\t\t},\r\n\t\t\tdatetimeEnd:function(newVal, oldVal){\r\n\t\t\t\tif(this.datetimeStart > this.datetimeEnd)\r\n\t\t\t\t{\r\n\t\t\t\t\tthis.datetimeEnd = this.datetimeStart;\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t},\r\n\t\tcreated(){\r\n\t\t\tthis.getReport();\r\n\t\t\tthis.getTable();\r\n\t\t\tthis.getProduct();\t\r\n\t\t},\r\n\t\tfilters: {\r\n\t\t\ttrimWords(value){\r\n\t\t\t\treturn value.split(\" \").splice(0,20).join(\" \") + '...';\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n\t.gosterim {\r\n\t\tfont-size: 16px;\r\n\t\tline-height: 34px;\r\n\t\ttext-align: center;\r\n\t\tcolor: #1c1c1c;\r\n\t\tpadding-right: 10px;\r\n\t}\r\n\r\n\t.fromTo {\r\n\t\tfont-size: 13px;\r\n\t\tcolor: #9a9494;\r\n\t\tpadding: 0 20px;\r\n\t\tbackground: #ffffff;\r\n\t}\r\n\r\n\t.fromTo:before {\r\n\t\tcontent: '';\r\n\t\twidth: 100%;\r\n\t\theight: 1px;\r\n\t\tbackground: #efeeee;\r\n\t\tposition: absolute;\r\n\t\tleft: 0;\r\n\t\ttop: 10px;\r\n\t\tz-index: -1;\r\n\t}\r\n</style>"],"sourceRoot":"src/components"}]}