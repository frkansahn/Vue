ate , 'order_user_id':this.orderProduct.order_user_id} , {'headers': {'Authorization':'Bearer ' + this.$store.state.token}})\r\n\t\t\t\t.then(response => {\r\n\t\t\t\t\tthis.isOrderAdd = false;\r\n\t\t\t\t\tthis.getTableDetail();\r\n\t\t\t\t}).catch(err => {\r\n\t\t\t\t\tconsole.log(err);\r\n\t\t\t\t});\r\n\t\t\t},\r\n\t\t\tgetTableDetail(){\r\n\t\t\t\taxios.get('http://localhost:4001/api/v1/tableDetail/'+this.$route.params.id ,{'headers': { 'Authorization':'Bearer ' + this.$store.state.token}})\r\n\t\t\t\t.then(response => {\r\n\t\t\t\t\tthis.tableDetail = response.data.response;\r\n\t\t\t\t\tvar totalPrice = 0;\r\n\t\t\t\t\tresponse.data.response.map(function(value,key){\r\n\t\t\t\t\t\ttotalPrice = totalPrice + Number(value.order_price);\r\n\t\t\t\t\t});\r\n\t\t\t\t\tthis.tableTotalPrice = totalPrice;\r\n\t\t\t\t}).catch(err => {\r\n\t\t\t\t\tconsole.log(err);\r\n\t\t\t\t});\r\n\r\n\t\t\t\taxios.get('http://localhost:4001/api/v1/product/' ,{'headers': { 'Authorization':'Bearer ' + this.$store.state.token}})\r\n\t\t\t\t.then(response => {\r\n\t\t\t\t\tthis.products = response.data.response;\r\n\t\t\t\t}).catch(err => {\r\n\t\t\t\t\tconsole.log(err);\r\n\t\t\t\t});\r\n\t\t\t}\r\n\t\t},\r\n\t\tcreated(){\r\n\t\t\tthis.getTableDetail();\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style scoped>\r\n\t.priceTitle , .priceValue {\r\n\t\tfont-size: 20px;\r\n\t\tfont-weight: normal;\r\n\t\tline-height: normal;\r\n\t\tcolor: #1c1c1c;\r\n\t}\r\n\r\n\t.go-home\r\n\t{\r\n\t\tcolor: #000000;\r\n\t\tmargin-top: 15px;\r\n\t\tcursor: pointer;\r\n\t}\r\n\r\n\t.proAdd {\r\n\t    font-size: 20px;\r\n\t    font-weight: 600;\r\n\t    line-height: normal;\r\n\t    color: #000000;\r\n\t    text-transform: uppercase;\r\n\t    padding-bottom: 20px;\r\n\t    display: block;\r\n\t}\r\n\r\n\t.no-order-table {\r\n\t    font-size: 26px;\r\n\t    color: #000000;\r\n\t    font-weight: 500;\r\n\t    line-height: normal;\r\n\t    text-decoration: underline;\r\n\t    margin-top: 2em;\r\n\t    display: block