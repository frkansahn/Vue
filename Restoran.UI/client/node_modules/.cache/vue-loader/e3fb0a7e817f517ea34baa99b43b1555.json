onent only -->\r\n<style scoped>\r\n\r\n\t.login\r\n\t{\r\n\t\tposition: fixed;\r\n\t\ttop: 15%;\r\n\t\tright: 0;\r\n\t\tleft: 0;\r\n\t\tz-index: 1030;\r\n\t}\r\n\r\n\t.form-control::placeholder\r\n\t{\r\n\t\tfont-size: 13px;\r\n\t}\r\n\r\n\t.loginBg\r\n\t{\r\n\t    position: absolute;\r\n\t    top: 0;\r\n\t    left: 0;\r\n\t    right: 0;\r\n\t    bottom: 0;\r\n\t    background-size: cover !important;\r\n\t}\r\n\r\n\t.btn-login\r\n\t{\r\n\t\tbackground: #ff9800;\r\n\t\tborder: 1px solid #ff9800;\r\n\t\tcolor: #ffffff;\r\n\t}\r\n\r\n\t.btn-login:hover\r\n\t{\r\n\t\tbackground: #f59406;\r\n\t\tborder: 1px solid #f59406;\r\n\t}\r\n\r\n\t@media screen and (max-width:540px)\r\n\t{\r\n\t\t.login\r\n\t\t{\r\n\t\t\ttop: 33%;\r\n\t\t}\r\n\t}\r\n</style>\r\n"]}]}