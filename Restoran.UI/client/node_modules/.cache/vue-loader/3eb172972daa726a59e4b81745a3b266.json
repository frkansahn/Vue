t\treturn this.reportOrders;\t\t\t\t\t\r\n\t\t\t\t}\r\n\t\t\t\telse if(this.selectTable != 0 && this.selectProduct == 0)\r\n\t\t\t\t{\r\n\t\t\t\t\treturn this.reportOrders.filter(order => {\r\n\t\t\t\t\t\treturn order.order_log_table_id == this.selectTable;\r\n\t\t\t\t\t});\r\n\t\t\t\t}\r\n\t\t\t\telse if(this.selectTable == 0 && this.selectProduct != 0)\r\n\t\t\t\t{\r\n\t\t\t\t\treturn this.reportOrders.filter(order => {\r\n\t\t\t\t\t\treturn order.order_log_product_id == this.selectProduct;\r\n\t\t\t\t\t});\r\n\t\t\t\t}\r\n\t\t\t\telse if(this.selectTable != 0 && this.selectProduct != 0)\r\n\t\t\t\t{\r\n\t\t\t\t\treturn this.reportOrders.filter(order => {\r\n\t\t\t\t\t\treturn order.order_log_product_id == this.selectProduct && order.order_log_table_id == this.selectTable;\r\n\t\t\t\t\t});\r\n\t\t\t\t}\r\n\t\t\t\t\r\n\t\t\t},\r\n\t\t\tdownloadOrder(){\r\n\r\n\t\t\t},\r\n\t\t\tprintOrder(){\r\n\t\t\t\tvar myWindow = window.open(\"\" , \"_blank\" , \"height=700,width=700\");\r\n\r\n\t\t\t\tvar tableHtml = this.$refs.printTable;\r\n    \t\t\tmyWindow.document.write(tableHtml.outerHTML);    \t\t\t\r\n    \t\t\tmyWindow.print();\r\n    \t\t\tmyWindow.close();\r\n\t\t\t}\r\n\t\t},\r\n\t\tcomputed: {\r\n\t\t\tdisplayedOrders () {\r\n\t\t\t\treturn this.paginate(this.orders);\r\n\t\t\t}\r\n\t\t},\r\n\t\twatch: {\r\n\t\t\torders () {\r\n\t\t\t\tthis.setPages();\r\n\t\t\t}\r\n\t\t},\r\n\t\tcreated(){\r\n\t\t\tthis.getReport();\r\n\t\t\tthis.getTable();\r\n\t\t\tthis.getProduct();\t\r\n\t\t},\r\n\t\tfilters: {\r\n\t\t\ttrimWords(value){\r\n\t\t\t\treturn value.split(\" \").splice(0,20).join(\" \") + '...';\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n\t.gosterim {\r\n\t\tfont-size: 16px;\r\n\t\tline-height: 34px;\r\n\t\ttext-align: center;\r\n\t\tcolor: #1c1c1c;\r\n\t\tpadding-right: 10px;\r\n\t}\r\n\r\n\t.fromTo {\r\n\t    font-size: 13px;\r\n\t    color: #9a9494;\r\n\t    padding: 0 20px;\r\n\t    background: #ffffff;\r\n\t}\r\n\r\n\t.fromTo:before 