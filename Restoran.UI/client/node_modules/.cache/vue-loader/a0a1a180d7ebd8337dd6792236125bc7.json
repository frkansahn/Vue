ent.contentDescription\" :config=\"editorConfig\" @ready=\"onReady\"></ckeditor>\r\n\t\t\t\t</div>\r\n\t\t\t\t<div class=\"row mt-3\">\r\n\t\t\t\t\t<div class=\"col-12 p-0\">\r\n\t\t\t\t\t\t<button class=\"btn btn-success btn-small px-3 py-1 float-right\" type=\"button\" name=\"save\" @click=\"fn_helpSave\">\r\n\t\t\t\t\t\t\tKaydet\r\n\t\t\t\t\t\t</button>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t<Footer/>\r\n\t</div>\r\n</template>\r\n<script>\r\n\timport Header from '@/components/Header'\r\n\timport Footer from '@/components/Footer'\r\n\timport axios from 'axios'\r\n\timport DecoupledEditor from '@ckeditor/ckeditor5-build-decoupled-document'\r\n\timport Swal from 'sweetalert2'\r\n\r\n\texport default{\r\n\t\tname:'Help',\r\n\t\tcomponents:{\r\n\t\t\tHeader , Footer\r\n\t\t},\r\n\t\tdata(){\r\n\t\t\treturn{\r\n\t\t\t\teditor: DecoupledEditor,\r\n                editorConfig: {\r\n                    // The configuration of the editor.\r\n                },\r\n                content:{\r\n                \tcontentName:'',\r\n                \tcontentDescription:'',\r\n                \tcreatedAt:'',\r\n                \tupdatedAt:''\r\n                }\r\n\t\t\t}\r\n\t\t},\r\n\t\tmethods:{\r\n\t\t\tnowDate:function(){\r\n\t\t\t\tvar date = new Date();\r\n\t\t\t\tvar year = date.getFullYear();\r\n\t\t\t\tvar year = date.getFullYear();\r\n\t\t\t\tvar mounth = String(date.getMonth() + 1);\r\n\t\t\t\tvar day = String(date.getDate());\r\n\t\t\t\tif (day.length == 1) {\r\n\t\t\t\t\tday = \"0\" + day;\r\n\t\t\t\t}\r\n\t\t\t\tif (mounth.length == 1) {\r\n\t\t\t\t\tmounth = \"0\" + mounth;\r\n\t\t\t\t}\r\n\r\n\t\t\t\tvar time = date.toLocaleTimeString();\r\n\r\n\t\t\t\tvar nowDate = year + '-' + mounth + '-' + day + ' ' + time;\r\n\t\t\t\treturn nowDate ;\r\n\t\t\t},\r\n\t\t\tfn_helpSave(){\r\n\t\t\t\tvar updateDate = this.nowDate();\r\n\t\t\t\tthis.content.updatedAt = updateDate;\r\n\t\t\t\tthis.content.createdAt = this.content.createdAt.replace('T',' ').split('.')[0];\r\n\t\t\t\tSwal.fire({\r\n\t\t\t\t\ttitle: 'Emin misin?',\r\n\t\t\t\t\ttext: \"Güncellemek istiyor musunuz?\",\r\n\t\t\t\t\ticon: 'question',\r\n\t\t\t\t\tshowCancelButton: true,\r\n\t\t\t\t\tconfirmButtonColor: '#3085d6',\r\n\t\t\t\t\tcancelButtonColor: '#d33',\r\n\t\t\t\t\tconfirmButtonText: 'Evet',\r\n\t\t\t\t\tcancelButtonText: 'Hayır'\r\n\t\t\t\t}).then((result) => {\r\n\t\t\t\t\tif (result.value == true) {\r\n\t\t\t\t\t\t\r\n\t\t\t\t\t\taxios.post('http://localhost:4001/api/v1/content/1',{'contentName':this.content.contentName , 'contentDescription':this.content.contentDescription , 'createdAt':this.content.createdAt , 'updateAt':this.content.updatedAt} ,{'headers': { 'Authorization':'Bearer ' + localStorage.getItem('token')}})\r\n\t\t\t\t\t\t.then(response => {\r\n\t\t\t\t\t\t\tif(!response.data.error)\r\n\t\t\t\t\t\t\t{\r\n\t\t\t\t\t\t\t\tSwal.fire(\r\n\t\t\t\t\t\t\t\t\t'',\r\n\t\t\t\t\t\t\t\t\t'Başarılı.',\r\n\t\t\t\t\t\t\t\t\t'success'\r\n\t\t\t\t\t\t\t\t\t);\r\n\t\t\t\t\t\t\t\tthis.getStart();\r\n\t\t\t\t\t\t\t}\r\n\t\t\t\t\t\t\telse\r\n\t\t\t\t\t\t\t{\r\n\t\t\t\t\t\t\t\tresponse.data.error.sqlMessage ? console.log(response.data.error.sqlMessage) : false;\r\n\t\t\t\t\t\t\t\tSwal.fire(\r\n\t\t\t\t\t\t\t\t\t'',\r\n\t\t\t\t\t\t\t\t\t'Başarısız.',\r\n\t\t\t\t\t\t\t\t\t'error'\r\n\t\t\t\t\t\t\t\t\t);\r\n\t\t\t\t\t\t\t}\r\n\t\t\t\t\t\t}).catch(err => {\r\n\t\t\t\t\t\t\tconsole.log(err);\r\n\t\t\t\t\t\t});\r\n\t\t\t\t\t}\r\n\t\t\t\t})\r\n\t\t\t},\r\n\t\t\tonReady( editor )  {\r\n\t\t\t\tvar ckToolbar = document.getElementById('ckToolbar');\r\n\t\t\t\tckToolbar.appendChild(editor.ui.view.toolbar.element);\r\n\t\t\t},\r\n\t\t\tgetStart(){\r\n\t\t\t\taxios.get('http://localhost:4001/api/v1/content' ,{'headers': { 'Authorization':'Bearer ' + localStorage.getItem('token')}})\r\n\t\t\t\t.then(response => {\r\n\t\t\t\t\tvar contentName = '';\r\n\t\t\t\t\tvar contentDescription = '';\r\n\t\t\t\t\tvar createdAt = '';\r\n\t\t\t\t\tvar updatedAt = ''\r\n\t\t\t\t\tresponse.data.response.map(function(val,key){\r\n\t\t\t\t\t\tif(val.contentId == 1)\r\n\t\t\t\t\t\t{\r\n\t\t\t\t\t\t\tcontentDescription = val.contentDescription;\r\n\t\t\t\t\t\t\tcontentName = val.contentName;\r\n\t\t\t\t\t\t\tcreatedAt = val.createdAt;\r\n\t\t\t\t\t\t\tupdatedAt = val.updateAt;\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t});\r\n\t\t\t\t\tthis.content.contentName = contentName;\r\n\t\t\t\t\tthis.content.contentDescription = contentDescription;\r\n\t\t\t\t\tthis.content.createdAt = createdAt;\r\n\t\t\t\t\tthis.content.updatedAt = updatedAt;\r\n\t\t\t\t}).catch(err => {\r\n\t\t\t\t\tconsole.log(err);\r\n\t\t\t\t});\r\n\t\t\t}\r\n\t\t},\r\n\t\tcreated(){\r\n\t\t\tthis.getStart();\r\n\t\t}\r\n\t}\r\n</script>\r\n<style scoped>\r\n\t.ck-editor__editable  {\r\n\t    -ms-flex: 0 0 100%;\r\n\t    flex: 0 0 100%;\r\n\t    max-width: 100%;\r\n\t    outline: none;\r\n\t    border: 1px solid #c4c4c4;\r\n\t}\r\n</style>"]}]}