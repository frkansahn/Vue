<td class=\"priceValue\">{{tableTotalPrice}} TL</td>\r\n\t\t\t\t\t\t\t<td></td>\r\n\t\t\t\t\t\t</tr>\r\n\t\t\t\t\t</tfoot>\r\n\t\t\t\t</table>\r\n\t\t\t\t<div v-else class=\"col-12 mt-2 mb-5\">\r\n\t\t\t\t\t<span class=\"no-order-table hvr-wobble-horizontal\">Masada Sipariş Bulunmamaktadır..!!</span>\r\n\t\t\t\t\t<div class=\"col-12 mt-3\">\r\n\t\t\t\t\t\t<div class=\"col-2 border rounded hover btn btn-success pay p-2\" @click=\"isOrderAdd = true\">Sipariş Ekle</div>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t\t<transition name=\"fade\">\r\n\t\t\t<div class=\"popup\" v-show=\"isOrderAdd\">\r\n\t\t\t\t<div class=\"popupContent col-5\">\r\n\t\t\t\t\t<div class=\"row mb-3\">\r\n\t\t\t\t\t\t<div class=\"col-12 mb-2\">\r\n\t\t\t\t\t\t\t<span class=\"proAdd\">Ürün Ekle</span>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t<div class=\"col-6\">\r\n\t\t\t\t\t\t\t<div class=\"row\">\r\n\t\t\t\t\t\t\t\t<span class=\"col-12 text-left\">Ürün</span>\r\n\t\t\t\t\t\t\t\t<div class=\"col-12\">\r\n\t\t\t\t\t\t\t\t\t<select class=\"col-12 form-control\" v-model=\"orderProduct.product_id\">\r\n\t\t\t\t\t\t\t\t\t\t<option value=\"\" disabled=\"disabled\" selected>Ürün Seçiniz</option>\r\n\t\t\t\t\t\t\t\t\t\t<option v-for=\"product in products\" v-bind:value=\"product.product_id\">{{product.product_name}}</option>\r\n\t\t\t\t\t\t\t\t\t</select>\r\n\t\t\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t<div class=\"col-6\">\r\n\t\t\t\t\t\t\t<div class=\"row\">\r\n\t\t\t\t\t\t\t\t<span class=\"col-12 text-left\">Adet</span>\r\n\t\t\t\t\t\t\t\t<div class=\"col-12\">\r\n\t\t\t\t\t\t\t\t\t<select class=\"col-12 form-control\" v-model=\"orderProduct.order_count\">\r\n\t\t\t\t\t\t\t\t\t\t<option value=\"\" disabled=\"disabled\" selected>Adet Seçiniz</option>\r\n\t\t\t\t\t\t\t\t\t\t<option value=\"1\">1</option>\r\n\t\t\t\t\t\t\t\t\t\t<option value=\"2\">2</option>\r\n\t\t\t\t\t\t\t\t\t\t<option value=\"3\">3</option>\r\n\t\t\t\t\t\t\t\t\t\t<option value=\"4